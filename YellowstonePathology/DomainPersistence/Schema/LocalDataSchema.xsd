	<xsd:element name="OrderComment" type="OrderCommentType" ms:relation="tblOrderComment" ms:key-fields="OrderCommentId"></xsd:element>
	<xsd:complexType name="OrderCommentType">
		<xsd:sequence>
			<xsd:annotation>
