<Test:ResultControl x:Class="YellowstonePathology.UI.Test.TCellNKProfileResultPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Test="clr-namespace:YellowstonePathology.UI.Test">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>

        <Border Grid.Row="0" BorderBrush="Transparent" BorderThickness="0" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"></Border>
        <TextBlock Grid.Column="0" Grid.Row="0" Margin="20,2,2,2" Text="{Binding Path=PageHeaderText}" VerticalAlignment="Center" Foreground="White" FontSize="16" FontWeight="Bold"></TextBlock>

        <Grid Grid.Row="1" Margin="5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <Border Grid.Row="0" Grid.Column="0" BorderBrush="Transparent" BorderThickness="0" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"></Border>

            <Grid Grid.Column="0" Margin="10">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="20"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                </Grid.RowDefinitions>

                <TextBlock Grid.Column="0" Grid.Row="0" Margin="0,3,0,0" FontWeight="Bold" FontSize="14">Options</TextBlock>

                <TextBlock Grid.Column="0" Grid.Row="3" Margin="5,3,0,0">                    
                    <Hyperlink Click="HyperLinkAcceptResults_Click">Accept Results</Hyperlink>
                </TextBlock>

                <TextBlock Name="TextBlockShowDocument" Grid.Column="0" Grid.Row="4" Margin="5,3,0,0">                    
                    <Hyperlink Click="HyperLinkShowDocument_Click">Show Document</Hyperlink>
                </TextBlock>

                <TextBlock Grid.Column="0" Grid.Row="5" Margin="5,3,0,5">
					<Hyperlink Click="HyperLinkFinalize_Click">Finalize</Hyperlink>
                </TextBlock>

                <TextBlock Grid.Column="0" Grid.Row="7" Margin="5,3,0,0">                    
                    <Hyperlink Click="HyperLinkUnacceptResults_Click">Unaccept Results</Hyperlink>
                </TextBlock>

                <TextBlock Name="TextBlockUnfinalResults" Grid.Column="0" Grid.Row="8" Margin="5,3,0,0">                    
                    <Hyperlink Click="HyperLinkUnfinalResults_Click">Unfinalize Results</Hyperlink>
                </TextBlock>

            </Grid>

            <ScrollViewer Grid.Column="1" Margin="10">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Column="0" Grid.Row="0" Margin="2,2,2,2" FontWeight="Bold">WBC:</TextBlock>
                    <TextBox Grid.Column="1" Grid.Row="0" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.WBC}" />

                    <TextBlock Grid.Column="0" Grid.Row="1" FontWeight="Bold" Margin="2,2,2,2">Lympocyte Percentage:</TextBlock>
                    <TextBox Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.LymphocytePercentage, Converter={StaticResource NullableDoubleConverter}}" />

                    <Grid Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2" Margin="5,5,5,5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Column="0" Grid.Row="0" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">Marker:</TextBlock>
                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Left">%</TextBlock>

                        <TextBlock Grid.Column="0" Grid.Row="1" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD3:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="1" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD3Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80" />
                        <TextBlock Grid.Column="2" Grid.Row="1" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>

                        <TextBlock Grid.Column="0" Grid.Row="2" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD4:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="2" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD4Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80" />
                        <TextBlock Grid.Column="2" Grid.Row="2" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>

                        <TextBlock Grid.Column="0" Grid.Row="3" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD8:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="3" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD8Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80"/>
                        <TextBlock Grid.Column="2" Grid.Row="3" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        
                        <TextBlock Grid.Column="0" Grid.Row="4" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD16:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="4" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD16Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80"/>
                        <TextBlock Grid.Column="2" Grid.Row="4" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        
                        <TextBlock Grid.Column="0" Grid.Row="5" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD19:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="5" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD19Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80"/>
                        <TextBlock Grid.Column="2" Grid.Row="5" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        
                        <TextBlock Grid.Column="0" Grid.Row="6" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD45:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="6" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD45Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80"/>
                        <TextBlock Grid.Column="2" Grid.Row="6" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        
                        <TextBlock Grid.Column="0" Grid.Row="7" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD56:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="7" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD56Percent, Converter={StaticResource NullableDoubleConverter}}" Width="80"/>
                        <TextBlock Grid.Column="2" Grid.Row="7" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                    </Grid>

                    <Grid Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="2" Margin="5,5,5,5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Column="0" Grid.Row="0" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">Cell Type:</TextBlock>
                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left" Grid.ColumnSpan="2">% of lymphocytes</TextBlock>
                        <TextBlock Grid.Column="3" Grid.Row="0" Margin="10,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">Absolute Count</TextBlock>

                        <TextBlock Grid.Column="0" Grid.Row="1" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD3 T-cells:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="1" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD3TPercent, Converter={StaticResource NullableDoubleConverter}}" />
                        <TextBlock Grid.Column="2" Grid.Row="1" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        <TextBlock Grid.Column="3" Grid.Row="1" Margin="10,2,2,2" Text="{Binding Path=PanelSetOrder.CD3TCount}" />

                        <TextBlock Grid.Column="0" Grid.Row="2" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD4 T-helper:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="2" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD4TPercent, Converter={StaticResource NullableDoubleConverter}}" />
                        <TextBlock Grid.Column="2" Grid.Row="2" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        <TextBlock Grid.Column="3" Grid.Row="2" Margin="10,2,2,2" Text="{Binding Path=PanelSetOrder.CD4TCount}" />

                        <TextBlock Grid.Column="0" Grid.Row="3" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD8 T-suppressor:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="3" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD8TPercent, Converter={StaticResource NullableDoubleConverter}}" />
                        <TextBlock Grid.Column="2" Grid.Row="3" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        <TextBlock Grid.Column="3" Grid.Row="3" Margin="10,2,2,2" Text="{Binding Path=PanelSetOrder.CD8TCount}" />

                        <TextBlock Grid.Column="0" Grid.Row="4" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD16 CD56 NK cells:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="4" Margin="2,2,0,2" Text="{Binding Path=PanelSetOrder.CD16CD56NKPercent, Converter={StaticResource NullableDoubleConverter}}" />
                        <TextBlock Grid.Column="2" Grid.Row="4" Margin="0,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">%</TextBlock>
                        <TextBlock Grid.Column="3" Grid.Row="4" Margin="10,2,2,2" Text="{Binding Path=PanelSetOrder.CD16CD56NKCount}" />

                        <TextBlock Grid.Column="0" Grid.Row="5" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">CD4/CD8 ratio:</TextBlock>
                        <TextBlock Grid.Column="1" Grid.Row="5" Margin="10,2,2,2" Text="{Binding Path=PanelSetOrder.CD4CD8Ratio}" />
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="4" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">Clinical History:</TextBlock>
                    <TextBox Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="2" Margin="2,2,2,2" Text="{Binding Path=AccessionOrder.ClinicalHistory}" TextWrapping="Wrap" AcceptsReturn="True"/>
                    
                    <TextBlock Grid.Column="0" Grid.Row="6" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">Method:</TextBlock>
                    <TextBox Grid.Column="0" Grid.Row="7" Grid.ColumnSpan="2" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.Method}" TextWrapping="Wrap" AcceptsReturn="True"/>

                    <TextBlock Grid.Column="0" Grid.Row="8" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">References:</TextBlock>
                    <TextBox Grid.Column="0" Grid.Row="9" Grid.ColumnSpan="2" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.ReportReferences}" TextWrapping="Wrap" AcceptsReturn="True" />

                    <TextBlock Grid.Column="0" Grid.Row="10" Margin="2,2,2,2" VerticalAlignment="Center" FontWeight="Bold" HorizontalAlignment="Left">ASR Comment:</TextBlock>
                    <TextBox Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="2" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.ASRComment}" TextWrapping="Wrap" AcceptsReturn="True"/>

                    <TextBlock Grid.Column="0" Grid.Row="12" Margin="2,2,2,2" FontWeight="Bold">Distribute:</TextBlock>
                    <CheckBox Grid.Column="1" Grid.Row="12" Margin="2,2,2,2" IsChecked="{Binding Path=PanelSetOrder.Distribute}"></CheckBox>

                    <TextBlock Grid.Column="0" Grid.Row="13" Margin="2,2,2,2" FontWeight="Bold">No Charge:</TextBlock>
                    <CheckBox Grid.Column="1" Grid.Row="13" Margin="2,2,2,2" IsChecked="{Binding Path=PanelSetOrder.NoCharge}"></CheckBox>

                    <Grid Grid.Column="0" Grid.Row="14" Grid.ColumnSpan="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Column="0" Grid.Row="0" Margin="2,2,2,2" FontWeight="Bold">Accepted:</TextBlock>
                        <CheckBox Grid.Column="1" Grid.Row="0" Margin="2,2,2,2" IsChecked="{Binding Path=PanelSetOrder.Accepted}" IsEnabled="False"></CheckBox>

                        <TextBlock Grid.Column="0" Grid.Row="1" Margin="2,2,2,2" FontWeight="Bold">Date Accepted:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.AcceptedTime}" IsEnabled="False"></TextBox>

                        <TextBlock Grid.Column="0" Grid.Row="2" Margin="2,2,2,2" FontWeight="Bold">Accepted By:</TextBlock>
                        <TextBox Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.AcceptedBy, Mode=OneWay}" IsEnabled="False"></TextBox>

                        <TextBlock Grid.Column="2" Grid.Row="0" Margin="2,2,2,2" FontWeight="Bold">Final:</TextBlock>
                        <CheckBox Grid.Column="3" Grid.Row="0" Margin="2,2,2,2" IsChecked="{Binding Path=PanelSetOrder.Final}" IsEnabled="False"></CheckBox>

                        <TextBlock Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" FontWeight="Bold">Date Finaled:</TextBlock>
                        <TextBox Grid.Column="3" Grid.Row="1" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.FinalTime}" IsEnabled="False"></TextBox>

                        <TextBlock Grid.Column="2" Grid.Row="2" Margin="2,2,2,2" FontWeight="Bold">Finaled By:</TextBlock>
                        <TextBox Grid.Column="3" Grid.Row="2" Margin="2,2,2,2" Text="{Binding Path=PanelSetOrder.Signature}" IsEnabled="False"></TextBox>

                    </Grid>

                </Grid>

            </ScrollViewer>

        </Grid>

        <Border Grid.Column="0" Grid.Row="2" BorderBrush="Transparent" BorderThickness="0" Background="{DynamicResource {x:Static SystemColors.InactiveCaptionBrushKey}}"></Border>
        <Button Name="ButtonNext" Grid.Column="0" Grid.Row="2" Width="100" Height="40" HorizontalAlignment="Right" Margin="5,5,5,5" Click="ButtonNext_Click">Next ></Button>

    </Grid>
</Test:ResultControl>
