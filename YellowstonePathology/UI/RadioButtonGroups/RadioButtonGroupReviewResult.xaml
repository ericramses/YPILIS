<UserControl x:Class="YellowstonePathology.UI.RadioButtonGroups.RadioButtonGroupReviewResult"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Horizontal">
		<RadioButton Name="radioButtonNotReviewed" Margin="3" Click="RadioButtonReviewResultSelection_Click"></RadioButton>
		<RadioButton Name="radioButtonAgree" Margin="3" Click="RadioButtonReviewResultSelection_Click"></RadioButton>
		<RadioButton Name="radioButtonDisagree" Margin="3" Click="RadioButtonReviewResultSelection_Click"></RadioButton>		
	</StackPanel>
</UserControl>
