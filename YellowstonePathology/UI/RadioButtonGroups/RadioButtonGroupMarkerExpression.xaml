<UserControl x:Class="YellowstonePathology.UI.RadioButtonGroups.RadioButtonGroupMarkerExpression"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Horizontal">
		<CheckBox Name="checkBoxMarkerSelected" Margin="3" Unchecked="CheckBoxMarkerSelected_Unchecked"></CheckBox>		
		<RadioButton Name="radioButtonExpresses" Click="RadioButtonExpresses_Click" Margin="3"></RadioButton>
		<RadioButton Name="radioButtonDoesNotExpress" Click="RadioButtonDoesNotExpress_Click" Margin="3"></RadioButton>
		<RadioButton Name="radioButtonEquivocal" Click="RadioButtonEquivocal_Click" Margin="3"></RadioButton>		
	</StackPanel>
</UserControl>
