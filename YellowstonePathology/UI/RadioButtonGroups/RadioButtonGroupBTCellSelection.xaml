<UserControl x:Class="YellowstonePathology.UI.RadioButtonGroups.RadioButtonGroupBTCellSelection"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
			<StackPanel Name="stackPanelBTCellSelection">
				<TextBlock Height="20"></TextBlock>
				<RadioButton Name="radioButtonBCells" Click="RadioButtonBTCellSelection_Click" Margin="3">B-Cells</RadioButton>
				<RadioButton Name="radioButtonTCells" Click="RadioButtonBTCellSelection_Click" Margin="3">T-Cells</RadioButton>
			</StackPanel>
    </StackPanel>
</UserControl>
