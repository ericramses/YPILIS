<UserControl x:Class="YellowstonePathology.UI.RadioButtonGroups.RadioButtonGroupLeukemiaCellDescription"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">    
		<StackPanel Name="stackPanelCellDescription">
			<TextBlock Margin="3" FontWeight="Bold">Cell Description</TextBlock>
			<RadioButton Name="radioButtonLymphocytes" Click="radioButtonGroupCellDescription_Click" Margin="3">Lymphocytes</RadioButton>
			<RadioButton Name="radioButtonMonocytes" Click="radioButtonGroupCellDescription_Click" Margin="3">Monocytes</RadioButton>
			<RadioButton Name="radioButtonMyeloidCells" Click="radioButtonGroupCellDescription_Click" Margin="3">Myeloid Cells</RadioButton>
			<RadioButton Name="radioButtonDIM45" Click="radioButtonGroupCellDescription_Click" Margin="3">DIM45</RadioButton>
			<RadioButton Name="radioButtonPlasmaCells" Click="radioButtonGroupCellDescription_Click" Margin="3">Plasma Cells</RadioButton>
			<RadioButton Name="radioButtonMyeloidBlast" Click="radioButtonGroupCellDescription_Click" Margin="3">Myeloid Blast</RadioButton>
		</StackPanel>
</UserControl>
