                    <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Margin="2,2,2,2" FontWeight="Bold" 
                               TextDecorations="Underline">Dual Stains</TextBlock>

                    <CheckBox Name="StainOrder01" Tag="k67smqntttvmlna" Grid.Column="0" Grid.Row="1" Margin="2,2,2,2"></CheckBox>
                    <TextBlock Grid.Column="1" Grid.Row="1" Margin="2,2,2,2">Ki-67/MelanA</TextBlock>

                    <CheckBox Name="StainOrder02" Tag="cytkrtn34p504srcmse" Grid.Column="0" Grid.Row="2" Margin="2,2,2,2"></CheckBox>
