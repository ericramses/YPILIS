<Window x:Class="YellowstonePathology.UI.ParsePsaAccessionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ParsePsaAccessionsWindow" Height="800" Width="700">
    <Grid Margin="5,5,5,5">
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>       
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        
        <Button Grid.Column="0" Grid.Row="0" Height="40" Margin="2,2,2,2" Click="ButtonParse_Click">Parse</Button>
        <TextBox Name="TextBoxText" Grid.Column="0" Grid.Row="1" Margin="2,2,2,2" AcceptsReturn="True" VerticalAlignment="Stretch" ScrollViewer.VerticalScrollBarVisibility="Visible"></TextBox>
        
    </Grid>
</Window>
